{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pre-commit": "lint-staged",
    "postinstall": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "start": "node build/index.js",
    "dev": "ts-node-dev  --respawn --transpile-only  src/index.ts",
    "dev:start": "concurrently \"npx tsc --watch\" \"nodemon -q build/index.js\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "lint-staged": {
    "*.ts": [
      "npm run format"
    ]
  },
  "_moduleAliases": {
    "$": "build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "concurrently": "^7.6.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
